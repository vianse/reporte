<p id="notice"><%= notice %></p>

<h1>Listing Sms</h1>

<table>
  <thead>
    <tr>
      <th>Telefono</th>
      <th>Mensaje</th>
      <th>Url</th>
      <th>Respuesta</th>
      <th>Comentario</th>
      <th>App</th>
      <th>Group</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sms.each do |sm| %>
      <tr>
        <td><%= sm.telefono %></td>
        <td><%= sm.mensaje %></td>
        <td><%= sm.url %></td>
        <td><%= sm.respuesta %></td>
        <td><%= sm.comentario %></td>
        <td><%= sm.app_id %></td>
        <td><%= sm.group_id %></td>
        <td><%= sm.user_id %></td>
        <td><%= link_to 'Show', sm %></td>
        <td><%= link_to 'Edit', edit_sm_path(sm) %></td>
        <td><%= link_to 'Destroy', sm, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sm', new_sm_path %>
