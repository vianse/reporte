<div id="content" class="content">
      <!-- begin breadcrumb -->
      <ol class="breadcrumb pull-right">
        <li><a href="javascript:;">Home</a></li>
        <li class="active">Periodos y objetivos</li>
      </ol>
      <!-- end breadcrumb -->
      <!-- begin page-header -->
      <h1 class="page-header">Periodos y objetivos <small></small></h1>
      <!-- end page-header -->
      

        <div class="panel panel-default">
          <div class="panel-heading">
            <h3 class="panel-title">Nuevo objetivo</h3>
          </div>
          <div class="panel-body">
           <%= form_for(@catalogo) do |f| %>
                <fieldset>
                <div class="form-group">
                  <%= f.label "Ingresa mes a configurar con formato numérico" %><br>
                  <%= f.text_field :mes,:placeholder => "Eje. 1 que corresponde a enero" , :class => "form-control",:value => Date.today.month %>
                </div>
              <div class="form-group">
                 <%= f.label "Año Actual" %><br>
                 <%= f.text_field :año,:value => Date.today.year,:placeholder => "Eje. 01 que corresponde a enero", :class => "form-control" %>
              </div>
              <div class="form-group">
                 <%= f.label "Ingresa tu objetivo obligado" %><br>
                 <%= f.text_field :objetivo_obligado, :placeholder => "Eje. 2000000", :class => "form-control" %>
              </div>
              <div class="form-group">
                 <%= f.label "Ingresa tu objetivo ideal" %><br>
                 <%= f.text_field :objetivo_real, :placeholder => "Eje. 2000000", :class => "form-control" %>
              </div>
              
              <div class="field">
                <%= f.hidden_field :app_id,:value => @parametro %>
                <%= f.hidden_field :user_id, :value => current_user.id %>
                <%= f.hidden_field :group_id, :value => @group_id %>
                <%= f.hidden_field :tipo, :value => @tipo %>
              </div>
              <div class="actions">
                 <%= f.submit "Guardar", :class => "btn btn-lg btn-success btn-block" %>
              </div>
            </fieldset>
            <% end -%>
              <hr/>
          </div>
      </div>

    
            <!-- begin #footer -->
            <div id="footer" class="footer">
                <span class="pull-right">
                    <a href="javascript:;" class="btn-scroll-to-top" data-click="scroll-top">
                        <i class="fa fa-arrow-up"></i> <span class="hidden-xs">Back to Top</span>
                    </a>
                </span>
